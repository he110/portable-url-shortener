{
    "name": "he110/portable-url-shortener",
    "description": "Standalone PHP library with Sqlite. Allows you to create and store links",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya S. Zobenko",
            "email": "ilya@zobenko.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "catfan/medoo": "^1.7",
        "psr/log": "^1.1",
        "duncan3dc/logger-aware-trait": "^0.2.0",
        "hashids/hashids": "^3.0"
    },
    "suggest": {
        "ext-bcmath": "Required to use BC Math arbitrary precision mathematics (*).",
        "ext-gmp": "Required to use GNU multiple precision mathematics (*)."
    },
    "autoload": {
        "psr-4": {"He110\\UrlShortener\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"He110\\UrlShortenerTests\\": "tests/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
